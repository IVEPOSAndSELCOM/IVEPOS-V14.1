<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:gravity="center_vertical">
    <ImageView
        android:id="@+id/search_item_icon"
        android:layout_width="wrap_content"
        android:paddingLeft="18dp"
        android:paddingRight="18dp"
        android:contentDescription="@string/action_item"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_search_item_grey_24dp" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_toLeftOf="@+id/select"
        android:layout_toRightOf="@+id/search_item_icon"
        android:orientation="horizontal"
        android:gravity="center_vertical"
        android:weightSum="10">
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1.5"
            android:gravity="left|center_vertical">
            <TextView
                android:id="@+id/iddd" android:textSize="15sp"
                android:textStyle="normal"
                android:textColor="#6a6a6d"
                android:paddingTop="15dp" android:paddingBottom="15dp"
                android:paddingLeft="10dp" android:paddingRight="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/textview" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="4.5"
            android:layout_height="wrap_content"
            android:gravity="left|center_vertical">
            <!--<TextView-->
            <!--android:layout_width="wrap_content"-->
            <!--android:layout_height="wrap_content"-->
            <!--android:textColor="#00a99d"-->
            <!--android:text="("/>-->
            <TextView
                android:id="@+id/cate" android:textSize="15sp"
                android:textStyle="bold" android:textColor="#00a99d"
                android:paddingTop="15dp" android:paddingBottom="15dp"
                android:paddingRight="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/textview" />
            <!--<TextView-->
            <!--android:layout_width="wrap_content"-->
            <!--android:layout_height="wrap_content"-->
            <!--android:textColor="#00a99d"-->
            <!--android:text=")"/>-->
            <TextView
                android:id="@+id/name" android:textSize="15sp"
                android:textStyle="bold"
                android:paddingTop="15dp" android:paddingBottom="15dp"
                android:paddingRight="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/textview" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/x"/>

            <TextView
                android:id="@+id/qty_value"
                android:layout_width="wrap_content" android:textSize="15sp"
                android:textStyle="normal"
                android:paddingTop="15dp" android:paddingBottom="15dp"
                android:layout_height="wrap_content" android:textColor="#6a6a6d"
                android:text="@string/textview" />

        </LinearLayout>

        <!--<LinearLayout-->
        <!--android:id="@+id/item_cate"-->
        <!--android:layout_width="0dp"-->
        <!--android:layout_weight="@string/Button11"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:gravity="left|center_vertical">-->
        <!--<TextView-->
        <!--android:id="@+id/cate" android:textSize="15sp"-->
        <!--android:textStyle="bold"-->
        <!--android:paddingTop="15dp" android:paddingBottom="15dp"-->
        <!--android:paddingLeft="10dp" android:paddingRight="10dp"-->
        <!--android:layout_width="wrap_content"-->
        <!--android:layout_height="wrap_content"-->
        <!--android:text="@string/textview" />-->
        <!--</LinearLayout>-->

        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="@string/Button10"
            android:orientation="horizontal"
            android:layout_height="wrap_content"
            android:gravity="end|center_vertical">
            <TextView
                android:layout_width="wrap_content" android:text="@string/textview"
                android:textSize="15sp" android:id="@+id/barvalue"
                android:paddingTop="15dp" android:paddingBottom="15dp"
                android:paddingLeft="10dp"
                android:layout_height="wrap_content" android:textColor="#6a6a6d"
                android:textStyle="normal"/>

        </LinearLayout>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_weight="@string/Button10"
            android:orientation="horizontal"
            android:layout_height="wrap_content"
            android:gravity="right|center_vertical">
            <TextView
                android:layout_width="wrap_content" android:text="@string/Rs" android:textSize="15sp"
                android:paddingTop="15dp" android:paddingBottom="15dp"
                android:paddingLeft="10dp"
                android:layout_height="wrap_content" android:textColor="#000000"
                android:textStyle="bold"/>
            <TextView
                android:id="@+id/code"
                android:layout_width="wrap_content" android:textSize="15sp"
                android:textStyle="bold"
                android:paddingTop="15dp" android:paddingBottom="15dp"
                android:layout_height="wrap_content" android:textColor="#000000"
                android:text="@string/textview" />
        </LinearLayout>
    </LinearLayout>

    <ImageView
        android:id="@+id/select"
        android:layout_width="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_search_launch_grey_24dp"
        android:padding="15dp"/>

</RelativeLayout>