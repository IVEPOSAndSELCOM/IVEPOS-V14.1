                android:gravity="center_horizontal"
                android:textColor="#000000"/>
            <TextView
                android:layout_width="match_parent" android:id="@+id/address3" android:fontFamily="sans-serif-thin"
                android:layout_height="wrap_content"
                android:text="Address line 3"
                android:textSize="10sp"
                android:gravity="center_horizontal"
                android:textColor="#000000"/>
            <TextView
                android:layout_width="match_parent" android:id="@+id/phoneno" android:fontFamily="sans-serif-thin"
                android:layout_height="wrap_content"
                android:text="Phone no."
                android:gravity="center"
                android:textSize="10sp"
                android:textColor="#000000"/>
            <TextView
                android:layout_width="match_parent" android:id="@+id/emailid" android:fontFamily="sans-serif-thin"
                android:layout_height="wrap_content"
                android:text="@string/email"
                android:gravity="center"
                android:textSize="10sp"
                android:textColor="#000000"/>
            <TextView
                android:layout_width="match_parent" android:id="@+id/website" android:fontFamily="sans-serif-thin"
                android:layout_height="wrap_content"
                android:text="Website"
                android:gravity="center"
                android:textSize="10sp"
                android:textColor="#000000"/>
            <TextView
                android:layout_width="match_parent" android:id="@+id/taxlineone"
