            android:layout_height="3dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/dotted_dashlines"/>

        <LinearLayout
            android:layout_width="match_parent" android:layout_marginTop="5dp"
            android:layout_height="wrap_content">


            <TextView
                android:text="@string/action_tax"
                android:layout_width="150dp" android:textSize="11.5sp"
                android:layout_height="wrap_content" android:fontFamily="sans-serif-thin"/>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:text="@string/Button13" android:id="@+id/tax" android:layout_gravity="end|center_vertical"
                    android:layout_width="wrap_content" android:textSize="11.5sp"
                    android:layout_height="wrap_content" android:fontFamily="sans-serif-thin"/>
            </FrameLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:text="Discount"
                android:layout_width="wrap_content" android:textSize="11.5sp"
                android:layout_height="wrap_content" android:fontFamily="sans-serif-thin"/>
            <TextView
                android:layout_width="wrap_content" android:text="("
                android:textSize="11.5sp"
                android:fontFamily="sans-serif-thin"
                android:layout_toRightOf="@+id/dis"
                android:layout_height="wrap_content"
                android:id="@+id/openbraces" android:textColor="#000000" />
            <TextView
                android:layout_width="wrap_content" android:text="@string/Button13"
                android:id="@+id/dispercent" android:textColor="#000000"
                android:layout_height="wrap_content"
                android:textSize="11.5sp"
                android:fontFamily="sans-serif-thin"
                android:layout_toRightOf="@+id/openbraces" />
            <TextView
                android:layout_width="wrap_content" android:text="%"
                android:id="@+id/percent" android:textColor="#000000"
                android:textSize="11.5sp"
                android:fontFamily="sans-serif-thin"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/dispercent"/>
            <TextView
                android:layout_width="wrap_content" android:text=")"
                android:id="@+id/closebraces" android:textColor="#000000"
                android:textSize="11.5sp"
                android:fontFamily="sans-serif-thin"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/percent" />


            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:text="@string/Button13" android:id="@+id/discount" android:layout_gravity="end|center_vertical"
                    android:layout_width="wrap_content" android:textSize="11.5sp"
                    android:layout_height="wrap_content" android:fontFamily="sans-serif-thin"/>
            </FrameLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">


            <TextView
                android:text="Savings" android:textSize="11.5sp"
                android:layout_width="150dp"
                android:layout_height="wrap_content" android:fontFamily="sans-serif-thin"/>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:text="@string/Button13" android:id="@+id/disc_savings"
                    android:layout_gravity="right|center_vertical"
                    android:layout_width="wrap_content" android:textSize="11.5sp"
                    android:layout_height="wrap_content" android:fontFamily="sans-serif-thin"/>
            </FrameLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent" android:layout_marginBottom="5dp"
            android:layout_height="wrap_content">


            <TextView
                android:text="Rounded" android:textSize="11.5sp"
                android:layout_width="150dp"
                android:layout_height="wrap_content" android:fontFamily="sans-serif-thin"/>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:text="@string/Button13" android:id="@+id/roundedoff"
                    android:layout_gravity="right|center_vertical"
                    android:layout_width="wrap_content" android:textSize="11.5sp"
                    android:layout_height="wrap_content" android:fontFamily="sans-serif-thin"/>
            </FrameLayout>
        </LinearLayout>

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginBottom="10dp"
            android:background="#7f7f7f"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:text="Total"
                android:layout_width="150dp" android:textSize="13sp"
                android:layout_height="wrap_content" android:fontFamily="sans-serif-thin"/>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:text="@string/Rs"
                    android:id="@+id/rs"
                    android:layout_toLeftOf="@+id/totalold"
                    android:layout_width="wrap_content" android:textSize="13sp"
                    android:layout_height="wrap_content" android:fontFamily="sans-serif-thin"/>
                <TextView
                    android:text="@string/Button13" android:id="@+id/totalold"
                    android:layout_alignParentRight="true"
                    android:layout_width="wrap_content" android:textSize="13sp"
                    android:layout_height="wrap_content" android:fontFamily="sans-serif-thin"/>
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent" android:orientation="vertical"
            android:layout_height="wrap_content" android:visibility="gone"
            android:id="@+id/foottt">
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="10dp"
                android:background="#7f7f7f"/>

            <TextView
                android:layout_width="match_parent" android:text="Thank you! come again soon."
                android:fontFamily="sans-serif-thin"
                android:gravity="center"
                android:textSize="11.5sp" android:layout_marginTop="15dp"
                android:layout_height="wrap_content" />

            <TextView
                android:layout_width="match_parent" android:text="Footer" android:gravity="start"
                android:textSize="10sp" android:layout_marginTop="15dp"
                android:id="@+id/footer" android:fontFamily="sans-serif-thin"
                android:layout_height="wrap_content" />
        </LinearLayout>




