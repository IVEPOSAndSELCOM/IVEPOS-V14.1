            android:id="@+id/address" android:visibility="gone"
            android:layout_height="wrap_content" android:layout_marginTop="5dp">
            <TextView
                android:layout_width="wrap_content" android:text="Customer"
                android:textSize="10sp" android:fontFamily="sans-serif-thin"
                android:layout_height="wrap_content" />
            <TextView
                android:layout_width="match_parent" android:id="@+id/cust_name"
                android:layout_height="wrap_content" android:text="Name"
                android:gravity="center"  android:fontFamily="sans-serif-thin"
                android:textSize="10sp"/>
            <TextView
                android:layout_width="match_parent" android:id="@+id/cust_address"
                android:layout_height="wrap_content" android:text="Name"
                android:gravity="center"  android:fontFamily="sans-serif-thin"
                android:textSize="10sp"/>
            <TextView
                android:layout_width="match_parent" android:id="@+id/cust_phno"
                android:layout_height="wrap_content" android:text="Name"
                android:gravity="center"  android:fontFamily="sans-serif-thin"
                android:textSize="10sp"/>
            <TextView
                android:layout_width="match_parent" android:id="@+id/cust_emailid"
                android:layout_height="wrap_content" android:text="Name"
                android:gravity="center"  android:fontFamily="sans-serif-thin"
                android:textSize="10sp"/>
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="5dp"
                android:background="#7f7f7f"/>
        </LinearLayout>

        <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent" android:weightSum="4.5"
            android:layout_height="wrap_content" android:gravity="center_vertical"
            android:orientation="horizontal"   >

            <TextView
                android:layout_width="0dp" android:id="@+id/name" android:text="Qty"
                android:layout_height="wrap_content" android:textSize="10sp"
                android:layout_marginTop="5dp" android:layout_marginBottom="5dp"
                android:fontFamily="sans-serif-thin" android:layout_weight="0.7"/>

            <TextView
                android:id="@+id/qty" android:text="@string/action_item" android:fontFamily="sans-serif-thin"
                android:layout_width="0dp"  android:textSize="10sp"
                android:layout_height="wrap_content" android:layout_weight="1.6"/>

            <TextView
                android:id="@+id/price" android:text="Price" android:fontFamily="sans-serif-thin"
                android:layout_width="0dp" android:textSize="10sp"
                android:gravity="end"
                android:layout_height="wrap_content" android:layout_weight="@string/Button9"/>

            <TextView
                android:id="@+id/amount" android:text="@string/amount" android:fontFamily="sans-serif-thin"
                android:layout_width="0dp" android:textSize="10sp" android:gravity="end"
                android:layout_height="wrap_content" android:layout_weight="1.2"/>



        </LinearLayout>
        <ImageView
