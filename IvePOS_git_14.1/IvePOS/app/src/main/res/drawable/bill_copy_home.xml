                android:text="Sub total"
                android:layout_width="150dp" android:textSize="11.5sp"
                android:layout_height="wrap_content" android:fontFamily="sans-serif-thin"/>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:text="@string/Button13" android:id="@+id/subtotal" android:layout_gravity="right|center_vertical"
                    android:layout_width="wrap_content" android:textSize="11.5sp"
                    android:layout_height="wrap_content" android:fontFamily="sans-serif-thin"/>
            </FrameLayout>

        </LinearLayout>

        <TableLayout
            android:id="@+id/lytpedidoo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" >
        </TableLayout>

        <ImageView
