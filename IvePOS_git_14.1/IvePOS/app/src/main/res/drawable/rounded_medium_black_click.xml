<?xml version="1.0" encoding="utf-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"  >
            <solid android:color="#b3b3b3" />
            <stroke android:width="1.0dp" android:color="#7f7f7f" />
            <corners android:radius="10.0dp" />
            <padding android:left="10.0dp" android:top="10.0dp" android:right="10.0dp" android:bottom="10.0dp" />
        </shape>


    </item>
    <item>
        <shape >
            <gradient android:startColor="#cccccc" android:endColor="#cccccc" android:angle="270.0" />
            <solid android:color="#cccccc" />
            <stroke android:width="1.0dp" android:color="#7f7f7f" />
            <corners android:radius="10.0dp" />
            <padding android:left="10.0dp" android:top="10.0dp" android:right="10.0dp" android:bottom="10.0dp" />

        </shape>
    </item>
</selector>