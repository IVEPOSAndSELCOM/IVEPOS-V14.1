<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent"
    android:gravity="center"
    android:paddingRight="1dp">
    <LinearLayout
        android:layout_width="wrap_content" android:layout_height="wrap_content"
        android:gravity="center"
        android:orientation="vertical"
        android:id="@+id/linearLayouthi"
        android:paddingRight="2dp"
        android:paddingLeft="2dp"
        android:paddingBottom="2dp"
        android:paddingTop="2dp"
        android:layout_centerInParent="true"
        android:padding="15dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">




        </RelativeLayout>

        <ImageView
            android:id="@+id/overflow"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:layout_toRightOf="@+id/table"
            android:src="@drawable/ic_more_vert_black_24dp" />

        <TextView
            android:layout_width="match_parent" android:visibility="gone"
            android:layout_height="match_parent"
            android:text="@string/floorname"
            android:textStyle="bold"
            android:textSize="15sp"
            android:gravity="center"
            android:id="@+id/floorname"/>

        <ImageView
            android:layout_width="wrap_content" android:id="@+id/table"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_table2"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text="@string/table"
                    android:textStyle="bold"
                    android:textSize="12sp"
                    android:gravity="left"
                    android:id="@+id/imageView1"
                    android:layout_weight="@string/Button9"/>

                <TextView
                    android:id="@+id/name"
                    android:textColor="#000000"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:lines="@string/Button9"
                    android:maxLines="@string/Button9"
                    android:text="@string/table_name"
                    android:textSize="12sp"
                    android:layout_centerHorizontal="true"
                    android:layout_weight="@string/Button9"
                    android:gravity="right"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent" android:visibility="gone"
                android:layout_height="match_parent"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text="@string/amount"
                    android:textStyle="bold"
                    android:textSize="15sp"
                    android:gravity="left"
                    android:id="@+id/imageView2"
                    android:layout_weight="@string/Button9"/>

                <TextView
                    android:id="@+id/amount"
                    android:textColor="#000000"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:lines="@string/Button9"
                    android:maxLines="@string/Button9"
                    android:text="@string/Button13"
                    android:textSize="15sp"
                    android:layout_centerHorizontal="true"
                    android:layout_weight="@string/Button9"
                    android:gravity="right"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text="@string/KOT"
                    android:textStyle="bold"
                    android:textSize="12sp"
                    android:gravity="left"
                    android:id="@+id/imageView4"
                    android:layout_weight="@string/Button9"/>

                <TextView
                    android:id="@+id/kot_status"
                    android:textColor="#000000"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:lines="@string/Button9"
                    android:maxLines="@string/Button9"
                    android:text="@string/Button13"
                    android:textSize="12sp"
                    android:layout_centerHorizontal="true"
                    android:layout_weight="@string/Button9"
                    android:gravity="right"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text="@string/pax"
                    android:textStyle="bold"
                    android:textSize="12sp"
                    android:gravity="left"
                    android:id="@+id/imageView3"
                    android:layout_weight="@string/Button9"/>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="@string/Button9"
                    android:gravity="right">
                    <TextView
                        android:id="@+id/no_of_pax"
                        android:textColor="#000000"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:lines="@string/Button9"
                        android:maxLines="@string/Button9"
                        android:text="@string/Button13"
                        android:textSize="12sp"
                        android:layout_centerHorizontal="true"
                        android:layout_gravity="right"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:text="/"
                        android:textSize="12sp"/>
                    <TextView
                        android:id="@+id/max_pax"
                        android:textColor="#000000"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:lines="@string/Button9"
                        android:maxLines="@string/Button9"
                        android:text="@string/Button13"
                        android:textSize="12sp"
                        android:layout_centerHorizontal="true"
                        android:layout_gravity="right"/>
                </LinearLayout>

            </LinearLayout>
        </LinearLayout>



    </LinearLayout>

</RelativeLayout>
