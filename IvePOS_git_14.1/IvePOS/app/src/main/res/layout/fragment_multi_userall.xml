<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:layout_width="match_parent" android:paddingTop="50dp" android:background="#e6e6e6"
    android:layout_height="match_parent" android:gravity="center_horizontal"
    android:id="@+id/relativeLayout2">

    <LinearLayout
        android:layout_width="wrap_content" android:orientation="vertical" android:padding="20dp"
        android:layout_height="wrap_content" android:background="#ffffff">
        <TextView
            android:layout_width="wrap_content" android:text="@string/admin_user_password" android:padding="10dp" android:textStyle="bold"
            android:layout_height="wrap_content" android:id="@+id/textView" android:textSize="15sp"/>
        <LinearLayout
            android:layout_width="wrap_content" android:gravity="center_vertical" android:background="#d0cfd0"
            android:layout_height="wrap_content" android:orientation="horizontal" android:layout_below="@+id/textView">
            <ImageView
                android:layout_width="wrap_content" android:background="#d0cfd0" android:padding="10dp"
                android:layout_height="wrap_content"
                android:src="@drawable/ic_password_black_24dp"/>
            <EditText
                android:layout_width="200dp" android:background="#d0cfd0" android:singleLine="true" android:password="true" android:id="@+id/etPass"
                android:layout_height="wrap_content" android:hint="Enter Password"
                android:imeOptions="actionGo"
                android:inputType="number" android:maxLength="@string/Button5" android:padding="10dp" android:textSize="15sp"/>
            <ImageView
                android:layout_width="wrap_content" android:src="@drawable/arrow_login" android:padding="10dp"
                android:layout_height="wrap_content" android:id="@+id/btngouser" android:background="@drawable/medium_black_click" />
        </LinearLayout>
    </LinearLayout>

</RelativeLayout>

