            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_gravity="end|center_vertical"
                    android:layout_width="wrap_content" android:textSize="11.5sp"
                    android:layout_height="wrap_content" android:fontFamily="sans-serif-thin"/>
            </FrameLayout>
        </LinearLayout>



