<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TableLayout
        android:id="@+id/tabla_cuerpo"
        android:layout_height="match_parent"
        android:layout_width="match_parent">
        <TableRow
            android:layout_width="match_parent" android:weightSum="6.8"
            android:layout_height="wrap_content">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:gravity="center_vertical"
                android:paddingLeft="5dp"
                android:paddingTop="6dp" android:paddingBottom="6dp"
                android:layout_weight="1.9">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="12sp"
                    android:paddingLeft="5dp"
                    android:gravity="center_vertical"
                    android:id="@+id/date"
                    android:text="@string/textview"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="12sp"
                    android:paddingLeft="5dp"
                    android:gravity="center_vertical"
                    android:id="@+id/time"
                    android:text="@string/textview"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                android:paddingTop="6dp" android:paddingBottom="6dp"
                android:layout_weight="1.2">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"

                    android:textSize="12sp"
                    android:paddingTop="6dp" android:paddingBottom="6dp"
                    android:gravity="center_vertical"
                    android:id="@+id/user"
                    android:text="@string/textview"/>
                <FrameLayout
                    android:layout_width="match_parent" android:visibility="invisible"
                    android:layout_height="match_parent"
                    >
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_gravity="center_vertical|right"
                        android:src="@drawable/ic_sort_24dp"
                        android:layout_height="wrap_content" />
                </FrameLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                android:paddingTop="6dp" android:paddingBottom="6dp"
                android:layout_weight="2.2">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"

                    android:textSize="12sp"
                    android:paddingTop="6dp" android:paddingBottom="6dp"
                    android:gravity="center_vertical"
                    android:id="@+id/expensename"
                    android:text="@string/textview"/>
                <FrameLayout
                    android:layout_width="wrap_content" android:visibility="invisible"
                    android:layout_height="match_parent"
                    >
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_gravity="center_vertical|right"
                        android:src="@drawable/ic_sort_24dp"
                        android:layout_height="wrap_content" />
                </FrameLayout>
            </LinearLayout>
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1.5"

                android:textSize="12sp"
                android:paddingLeft="5dp" android:paddingRight="5dp"
                android:gravity="center_vertical"
                android:id="@+id/price"
                android:text="@string/textview"/>

            <TextView
                android:layout_width="0dp" android:visibility="gone"
                android:layout_height="match_parent"
                android:layout_weight="@string/Button9"

                android:textSize="12sp"
                android:paddingLeft="5dp" android:paddingRight="5dp"
                android:paddingTop="6dp" android:paddingBottom="6dp"
                android:gravity="center_vertical"
                android:id="@+id/id"
                android:text="@string/textview"/>

        </TableRow>
    </TableLayout>
</LinearLayout>

