<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TableLayout
        android:id="@+id/tabla_cuerpo"
        android:layout_height="match_parent"
        android:layout_width="match_parent">
        <TableRow
            android:layout_width="match_parent" android:weightSum="6.8"
            android:layout_height="wrap_content">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                android:layout_weight="@string/Button11"
                android:paddingLeft="5dp" android:paddingRight="5dp"
                android:paddingTop="6dp" android:paddingBottom="6dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"

                    android:textSize="12sp"

                    android:gravity="center_vertical"
                    android:id="@+id/user"
                    android:text="@string/action_user"/>
            </LinearLayout>
            <TextView
                android:layout_width="0dp" android:id="@+id/no_of_transactions"
                android:layout_height="match_parent"
                android:layout_weight="1.9"
                android:paddingLeft="5dp" android:paddingRight="5dp"
                android:paddingTop="8dp" android:paddingBottom="8dp"
                android:textSize="12sp"
                android:gravity="center_vertical"
                android:text="@string/no_of_transactions"/>
            <TextView
                android:layout_width="0dp" android:id="@+id/amount"
                android:layout_height="match_parent"
                android:layout_weight="1.9"
                android:paddingLeft="5dp" android:paddingRight="5dp"
                android:paddingTop="8dp" android:paddingBottom="8dp"
                android:textSize="12sp"
                android:gravity="center_vertical"
                android:text="@string/amount"/>


        </TableRow>
    </TableLayout>
</LinearLayout>

