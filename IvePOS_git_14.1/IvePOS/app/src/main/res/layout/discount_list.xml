<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TableLayout
        android:id="@+id/tabla_cuerpo"
        android:layout_height="match_parent"
        android:layout_width="match_parent">
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="8.5">
<!--            <TextView-->
<!--                android:layout_width="0dp"-->
<!--                android:layout_height="match_parent"-->
<!--                android:layout_weight="0.8"-->
<!--                -->
<!--                android:paddingLeft="5dp" android:paddingRight="5dp"-->
<!--                android:paddingTop="12dp" android:paddingBottom="12dp"-->
<!--                android:gravity="start|center_vertical"-->
<!--                android:id="@+id/sno"-->
<!--                android:text="@string/textview"/>-->
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1.1"
                
                android:paddingLeft="5dp" android:paddingRight="5dp"
                android:paddingTop="12dp" android:paddingBottom="12dp"
                android:gravity="start|center_vertical"
                android:id="@+id/date"
                android:text="@string/textview"/>
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1.2"
                
                android:paddingLeft="5dp" android:paddingRight="5dp"
                android:paddingTop="12dp" android:paddingBottom="12dp"
                android:gravity="start|center_vertical"
                android:id="@+id/time"
                android:text="@string/textview"/>
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1.8"
                
                android:paddingLeft="5dp" android:paddingRight="5dp"
                android:paddingTop="12dp" android:paddingBottom="12dp"
                android:gravity="start|center_vertical"
                android:id="@+id/billno"
                android:text="@string/textview"/>
            <TextView
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1.3"
                
                android:paddingLeft="5dp" android:paddingRight="5dp"
                android:paddingTop="12dp" android:paddingBottom="12dp"
                android:gravity="start|center_vertical"
                android:id="@+id/discountcode"
                android:text="@string/textview"/>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="@string/Button9"
                android:orientation="horizontal">
                <TextView
                    android:layout_width="wrap_content" android:layout_height="match_parent"
                    
                    android:paddingLeft="5dp" android:paddingRight="3dp"
                    android:paddingTop="12dp" android:paddingBottom="12dp"
                    android:gravity="start|center_vertical"
                    android:id="@+id/discountpercent"
                    android:text="@string/_99_9"/>
                <TextView
                    android:layout_width="wrap_content" android:layout_height="match_parent"
                    android:paddingRight="5dp"
                    android:paddingTop="12dp" android:paddingBottom="12dp"
                    android:gravity="center_vertical"
                    android:text="%"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1.1">
                <TextView
                    android:layout_width="wrap_content" android:layout_height="match_parent"
                    android:id="@+id/inn"
                    android:paddingLeft="5dp" android:paddingRight="3dp"
                    android:paddingTop="12dp" android:paddingBottom="12dp"
                    android:gravity="center_vertical"
                    android:text="@string/insert"/>
                <TextView
                    android:layout_width="match_parent" android:layout_height="match_parent"
                    
                    android:paddingRight="5dp"
                    android:paddingTop="12dp" android:paddingBottom="12dp"
                    android:gravity="start|center_vertical"
                    android:id="@+id/billamount"
                    android:text="@string/bill"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="@string/Button9"
                >
                <TextView
                    android:layout_width="wrap_content" android:layout_height="match_parent"
                    android:id="@+id/inn1"
                    android:paddingLeft="5dp" android:paddingRight="3dp"
                    android:paddingTop="12dp" android:paddingBottom="12dp"
                    android:gravity="center_vertical"
                    android:text="@string/insert"/>
                <TextView
                    android:layout_width="match_parent" android:layout_height="match_parent"
                    
                    android:paddingRight="5dp"
                    android:paddingTop="12dp" android:paddingBottom="12dp"
                    android:gravity="start|center_vertical"
                    android:id="@+id/discountrs"
                    android:text="@string/subtitle14"/>
            </LinearLayout>
        </TableRow>
    </TableLayout>
</LinearLayout>