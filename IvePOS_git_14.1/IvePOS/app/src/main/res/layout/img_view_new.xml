<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="fill_parent"
    android:paddingLeft="5dp" android:paddingRight="5dp" android:background="#ffffff">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:paddingBottom="5dp"
        android:background="#ffffff">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:visibility="visible"
            android:orientation="vertical"
            android:id="@+id/complogo">
            <ImageView
                android:layout_width="match_parent" android:id="@+id/viewImagee"
                android:layout_height="wrap_content"
                android:layout_gravity="center" android:background="#ffffff"/>

        </LinearLayout>
    </LinearLayout>




</RelativeLayout>


