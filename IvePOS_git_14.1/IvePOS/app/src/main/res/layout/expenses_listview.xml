                        <ImageView
                            android:layout_width="wrap_content" android:id="@+id/arrow_expense"
                            android:layout_gravity="center_vertical|right"
                            android:src="@drawable/ic_arrow_drop_down_black_24dp"
                            android:layout_height="wrap_content" />
                    </FrameLayout>
                </LinearLayout><LinearLayout
                android:layout_width="0dp" android:id="@+id/amount_click"
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                android:layout_weight="1.5"
                android:background="#e6e6e6"
                android:paddingTop="6dp" android:paddingBottom="6dp">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"

                    android:textSize="12sp"

                    android:gravity="center_vertical"
                    android:id="@+id/amount"
                    android:text="@string/amount"
                    android:textStyle="bold"/>
                <FrameLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    >
                    <ImageView
                        android:layout_width="wrap_content" android:id="@+id/arrow_amount"
                        android:layout_gravity="center_vertical|right"
                        android:src="@drawable/ic_arrow_drop_down_black_24dp"
