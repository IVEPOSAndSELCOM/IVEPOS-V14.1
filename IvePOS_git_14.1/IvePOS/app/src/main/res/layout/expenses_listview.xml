            <ImageView
                android:layout_width="match_parent" android:id="@+id/imageView1"
                android:layout_height="1dp" android:layout_below="@+id/list_header"
                android:background="#7f7f7f"/>
            <LinearLayout
                android:layout_width="match_parent" android:layout_below="@+id/imageView1"
                android:visibility="visible" android:orientation="vertical"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_width="match_parent" android:text="" android:textSize="1sp"
                    android:layout_height="wrap_content" />
                <ListView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:id="@+id/listView"/>
