                    android:text="@string/Rs"
                    android:id="@+id/rs"
                    android:layout_toLeftOf="@+id/fulltotal1"
                    android:layout_width="wrap_content" android:textSize="13sp"
                    android:layout_height="wrap_content" android:fontFamily="sans-serif-thin"/>
                <TextView
                    android:text="@string/Button13" android:id="@+id/fulltotal1"
                    android:layout_alignParentRight="true"
                    android:layout_width="wrap_content" android:textSize="13sp"
                    android:layout_height="wrap_content" android:fontFamily="sans-serif-thin"/>
            </LinearLayout>
        </LinearLayout>

