            <TextView
                android:text="@string/rounded" android:textSize="11.5sp"
                android:layout_width="150dp"
                android:layout_height="wrap_content" android:fontFamily="sans-serif-thin"/>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
