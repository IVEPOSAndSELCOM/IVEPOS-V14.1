    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent" android:orientation="vertical"
        android:layout_height="wrap_content"
        android:paddingLeft="15dp" android:paddingRight="15dp" android:paddingBottom="10dp">
        <LinearLayout
            android:layout_width="match_parent" android:orientation="horizontal"
            android:layout_height="wrap_content" android:gravity="center_vertical">
            <TextView
                android:layout_width="140dp" android:text="@string/weekdays_reset_qty" android:textColor="#000000"
                android:layout_height="wrap_content"  />
            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:padding="1dp">
                <EditText
                    android:layout_width="match_parent"
                    android:id="@+id/editText1"
                    android:inputType="number"
                    android:hint="Enter qty"
                    android:selectAllOnFocus="true"
                    android:nextFocusDown="@id/editText2"
                    android:layout_height="wrap_content" android:singleLine="true"
                    android:ellipsize="end" android:maxLines="@string/Button9"/>
            </FrameLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent" android:orientation="horizontal" android:layout_marginTop="2dp"
            android:layout_height="wrap_content" android:gravity="center_vertical">
            <TextView
                android:layout_width="140dp" android:text="@string/weekends_reset_qty" android:textColor="#000000"
                android:layout_height="wrap_content" />
            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:padding="1dp">
                <EditText
                    android:layout_width="match_parent"
                    android:id="@+id/editText2"
                    android:inputType="number"
                    android:imeOptions="actionGo"
                    android:hint="Enter qty"
                    android:selectAllOnFocus="true"
                    android:nextFocusForward="@id/editText3"
                    android:layout_height="wrap_content"
                    android:singleLine="true"
                    android:ellipsize="end"
                    android:maxLines="@string/Button9"/>
            </FrameLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent" android:orientation="horizontal" android:layout_marginTop="2dp"
            android:layout_height="wrap_content" android:gravity="center_vertical">
            <TextView
                android:layout_width="140dp" android:text="@string/action_category" android:textColor="#000000"
                android:layout_height="wrap_content"  />
            <TextView
                android:layout_width="180dp" android:padding="10dp"
                android:id="@+id/categoryget"
                android:inputType="numberDecimal"
                android:nextFocusForward="@id/editText3"
                android:layout_height="wrap_content" android:singleLine="true"
                android:layout_marginLeft="10dp" android:ellipsize="end" android:maxLines="@string/Button9"/>
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent" android:orientation="horizontal" android:layout_marginTop="2dp"
            android:layout_height="wrap_content" android:gravity="center_vertical">
            <TextView
                android:layout_width="140dp" android:text="@string/item_no" android:textColor="#000000"
                android:layout_height="wrap_content" />
            <TextView
                android:layout_width="180dp" android:padding="10dp" android:id="@+id/editText4"
                android:layout_height="wrap_content" android:singleLine="true" android:textColor="#000000"
                android:layout_marginLeft="10dp" android:ellipsize="end" android:maxLines="@string/Button9"/>
        </LinearLayout>


        <!--<TextView-->
