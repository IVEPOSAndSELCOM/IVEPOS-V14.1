<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TableLayout
        android:id="@+id/tabla_cuerpo"
        android:layout_height="match_parent"
        android:layout_width="match_parent">
        <TableRow
            android:layout_width="match_parent" android:weightSum="8.5"
            android:layout_height="wrap_content">
            <TextView
                android:layout_width="0dp" android:id="@+id/dateget"
                android:layout_height="match_parent"
                android:layout_weight="0.9"
                android:paddingLeft="5dp" android:paddingRight="5dp"
                android:paddingTop="12dp" android:paddingBottom="12dp"
                android:gravity="start|center_vertical"
                
                android:text="@string/date"
                 />
            <TextView
                android:layout_width="0dp" android:id="@+id/timeget"
                android:layout_height="match_parent"
                android:layout_weight="1.1"
                android:paddingLeft="5dp" android:paddingRight="5dp"
                android:paddingTop="12dp" android:paddingBottom="12dp"
                android:gravity="start|center_vertical"
                
                android:text="@string/time_title"
                 />
            <TextView
                android:layout_width="0dp" android:id="@+id/userget"
                android:layout_height="match_parent"
                android:layout_weight="1.1"
                android:paddingLeft="5dp" android:paddingRight="5dp"
                android:paddingTop="12dp" android:paddingBottom="12dp"
                android:gravity="start|center_vertical"
                
                android:text="@string/action_user"
                 />
            <TextView
                android:layout_width="0dp" android:id="@+id/billcount"
                android:layout_height="match_parent"
                android:layout_weight="0.8"
                android:paddingLeft="5dp" android:paddingRight="5dp"
                android:paddingTop="12dp" android:paddingBottom="12dp"
                android:gravity="start|center_vertical"

                android:text="@string/bill_count"
                />
            <TextView
                android:layout_width="0dp" android:id="@+id/billnoget"
                android:layout_height="match_parent"
                android:layout_weight="1.5"
                android:paddingLeft="5dp" android:paddingRight="5dp"
                android:paddingTop="12dp" android:paddingBottom="12dp"
                android:gravity="start|center_vertical"
                
                android:text="@string/bill_no"
                 />
            <LinearLayout
                android:layout_width="0dp" 
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                android:layout_weight="0.9">
                <TextView
                    android:layout_width="wrap_content" android:layout_height="match_parent"
                    android:id="@+id/inn"
                    android:paddingLeft="5dp" android:paddingRight="3dp"
                    android:paddingTop="12dp" android:paddingBottom="12dp"
                    android:gravity="center_vertical"
                    android:text="@string/insert"/>
                <TextView
                    android:layout_width="wrap_content" android:layout_height="match_parent"
                    
                    android:paddingRight="5dp"
                    android:paddingTop="12dp" android:paddingBottom="12dp"
                    android:gravity="center_vertical"
                    android:id="@+id/salesget"
                    android:text="@string/sale"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp" 
                android:layout_height="match_parent"
                android:orientation="horizontal"
                android:gravity="center_vertical"
                android:layout_weight="0.9">
                <TextView
                    android:layout_width="wrap_content" android:layout_height="match_parent"
                    android:id="@+id/inn1"
                    android:paddingLeft="5dp" android:paddingRight="3dp"
                    android:paddingTop="12dp" android:paddingBottom="12dp"
                    android:gravity="center_vertical"
                    android:text="@string/insert"/>
                <TextView
                    android:layout_width="wrap_content" android:layout_height="match_parent"
                    
                    android:paddingRight="5dp"
                    android:paddingTop="12dp" android:paddingBottom="12dp"
                    android:gravity="center_vertical"
                    android:id="@+id/refundget"
                    android:text="@string/refund"/>
            </LinearLayout>
            
            <!--<TextView-->
                <!--android:layout_width="0dp"-->
                <!--android:layout_height="match_parent"-->
                <!--android:layout_weight="1.2"-->
                <!--android:paddingLeft="5dp" android:paddingRight="5dp"-->
                <!--android:paddingTop="8dp" android:paddingBottom="8dp"-->
                <!--android:gravity="start|center_vertical"-->
                <!---->
                <!--android:text="@string/refund"-->
                <!-- />-->
            <TextView
                android:layout_width="0dp" android:id="@+id/reasonget"
                android:layout_height="match_parent"
                android:layout_weight="1.3"
                android:paddingLeft="5dp" android:paddingRight="5dp"
                android:paddingTop="12dp" android:paddingBottom="12dp"
                android:gravity="start|center_vertical"
                
                android:text="@string/reason"
                 />

        </TableRow>
    </TableLayout>
</LinearLayout>

