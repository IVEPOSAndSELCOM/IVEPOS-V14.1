<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/relativeLayout1" android:layout_width="wrap_content" android:layout_height="wrap_content" android:background="#e6e6e6"
    xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical">
    <RelativeLayout
        android:layout_width="320dp" android:gravity="center_vertical"
        android:layout_height="wrap_content"
        android:background="#e6e6e6"
        android:layout_marginBottom="5dp">
        <Button
            android:id="@+id/cancel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:background="@drawable/light_black_click_shape_rounded_corners"
            android:src="@drawable/ic_arrow_back_black_24dp"
            android:padding="10dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="5dp"
            android:text="@string/subscription_prompt_cancel"
            android:textStyle="bold"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="@string/tab"
            android:layout_centerVertical="true"
            android:layout_centerHorizontal="true"
            android:layout_centerInParent="true"
            android:textSize="18sp"
            android:textStyle="bold"/>
        <Button
            android:id="@+id/enter"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="5dp"
            android:src="@drawable/ic_done_white_24dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@drawable/green_click_shape_rounded_corners"
            android:textStyle="bold"
            android:padding="10dp"
            android:text="@string/save"/>
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="horizontal"
        android:layout_height="wrap_content"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:gravity="center_vertical"
        android:paddingBottom="15dp">

        <LinearLayout
            android:layout_width="54dp"
            android:orientation="horizontal"
            android:layout_margin="10dp"
            android:padding="10dp"
            android:background="@drawable/light_black_click_shape_circle"
            android:layout_height="54dp"
            android:gravity="center" >
            <ImageView
                android:layout_width="wrap_content"
                android:textColor="#000000"
                android:src="@drawable/empty_table"
                android:layout_height="wrap_content"
                android:id="@+id/get_image" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="match_parent"
                android:orientation="horizontal"
                android:layout_height="wrap_content"
                android:gravity="center_vertical">

                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <EditText
                        android:layout_width="match_parent"
                        android:id="@+id/etName"
                        android:layout_height="wrap_content"
                        android:inputType="text"
                        android:maxLines="@string/Button9"
                        android:hint="@string/tab_name"
                        android:singleLine="true"
                        android:textSize="15sp"/>
                </com.google.android.material.textfield.TextInputLayout>
            </RelativeLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:layout_height="wrap_content"
                android:gravity="center_vertical">
                <TextView
                    android:layout_width="wrap_content"
                    android:hint="Tab no."
                    android:id="@+id/table_no"
                    android:paddingLeft="4dp"
                    android:layout_marginBottom="8dp"
                    style="@style/TextAppearance.Design.Hint"
                    android:layout_centerVertical="true"
                    android:layout_height="wrap_content"  />
                <TextView
                    android:layout_width="match_parent"
                    android:id="@+id/editText2"
                    android:layout_marginLeft="5dp"
                    android:layout_marginBottom="5dp"
                    android:layout_below="@+id/table_no"
                    android:layout_height="wrap_content"
                    android:singleLine="true"
                    android:textColor="#000000"
                    android:text="@string/_25"
                    android:textSize="15sp"
                    android:ellipsize="end"
                    android:maxLines="@string/Button9"/>
            </LinearLayout>

        </LinearLayout>

    </LinearLayout>


</LinearLayout>