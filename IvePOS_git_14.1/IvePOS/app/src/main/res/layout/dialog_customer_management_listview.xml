                        android:text="@string/R"
                        android:background="@drawable/light_black_rounded_corners"
                        android:layout_centerVertical="true"
                        android:layout_alignParentRight="true"
                        android:textSize="12sp"
                        android:gravity="center"
                        android:textStyle="bold"
                        android:layout_width="16dp"
                        android:layout_height="16dp"/>

                    <LinearLayout
                        android:layout_toLeftOf="@+id/ri_text"
                        android:layout_width="wrap_content"
                        android:minWidth="36dp"
                        android:layout_centerVertical="true"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:gravity="end">

                        <TextView
                            android:text="@string/Rs" android:id="@+id/rs_refunds_val"
                            android:textSize="14sp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>

                        <TextView
                            android:id="@+id/refunds_val"
                            android:text="@string/Button13"
                            android:textSize="14sp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>

                    </LinearLayout>

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/charges1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
